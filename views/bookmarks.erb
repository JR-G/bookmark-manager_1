<ul>
  <% @bookmarks.each do |bookmark| %>
    <li>
      <a href="<%= bookmark.url %>">
        <%= bookmark.title %>
      </a>
    </li>
  <% end %>
</ul>

<form action='/add' method='post'>
  <label for="url">
    URL:
    <input type="text" name="url">
  </label>
  <label for="title">
    Title:
    <input type="text" name="title">
  </label>
  <input type="submit" id="Add" value="Add to Bookmarks">
</form>
